	// Attributes
	private float currentPressure = 29.92f;
	private float lastPressure;
	private Subject weatherData;

	public ForecastDisplay(Subject weaterData) {
		this.weatherData = weatherData;
		weatherData.registerObserver(this);
	}
	
	// update arguments
	public void update() {
		
	}

}
